{
  "name": "appscript-chat-knowledge",
  "version": "0.1.0",
  "description": "Google Apps Script Chat bot for knowledge management.",
  "private": true,
  "type": "module",
  "scripts": {
    "login": "clasp login --no-localhost",
    "push": "clasp push",
    "pull": "clasp pull",
    "deploy": "clasp deploy --description \"Deploy from CI\"",
    "open": "clasp open",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "@google/clasp": "3.0.6-alpha"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nock": "^13.5.0"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "json",
      "gs"
    ]
  }
}
